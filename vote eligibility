#include <iostream>
using namespace std;
int main()
{
   int age;
   cout<<"Enter the age : ";
   cin>>age;
   if (age>=18)
   {
   cout<<"You are eligible for voting";
   }
   else
   {
   cout<<"You are eligible for voting";
   }
    return 0;
}

output :
 Ente the age : 19
 you are eigible for voting
